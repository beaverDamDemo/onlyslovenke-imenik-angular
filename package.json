{
  "name": "onlyslovenke-imenik-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart:dev": "node src/environments/set-env.js",
    "prestart:prod": "node src/environments/set-env.js",
    "prestart:build": "node src/environments/set-env.js",
    "start": "ng serve --configuration=development",
    "start:dev": "ng serve --configuration=development",
    "start:prod": "ng serve --configuration=production",
    "build": "npm run prestart:build && ng build --configuration=production",
    "github-build": "ng build --configuration production --base-href '/onlyslovenke-imenik-angular/'",
    "github-deploy": "npx angular-cli-ghpages --dir=dist/onlyslovenke-imenik-angular/browser --no-silent",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/jasmine": "~5.1.0",
    "colors": "^1.4.0",
    "dotenv": "^17.2.2",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.2"
  }
}
